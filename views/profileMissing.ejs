<!DOCTYPE HTML>
<html>
<head>
	<title>AYUDA</title>
<!-- STYLES & JQUERY 
	================================================== -->
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
	<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">
	<link rel="stylesheet" type="text/css" href="/assets/css/style.css"/>
	<link rel="stylesheet" type="text/css" href="/assets/css/icons.css"/>
	<link rel="stylesheet" type="text/css" href="/assets/css/skinblue.css"/><!-- Change skin color here -->
	<link rel="stylesheet" type="text/css" href="/assets/css/responsive.css"/>

</head>
</head>
<body>
	<div class="boxedtheme">

<!-- TOP LOGO & MENU
	================================================== -->
	<% include partials/header.ejs %>
	<!-- ================/TOP LOGO & MENU============= -->

<!-- SUBHEADER
	================================================== -->
	<% include partials/sub_header.ejs %>
	<!-- ================/SUBHEADER=================== -->

<!-- CONTENT
	================================================== -->
	<div class="grid" style="padding-top:1%;">
		<div class="row .mx-auto d-flex justify-content-center space-bot">
			<!--INTRO-->
			<div class="c12">
				<div class="royalcontent">
					<h1 class="royalheader">MISSING</h1>
				</div>
			</div>
			<!--Box 1-->
			<div class="c5">
				<div class="card noshadowbox">
					<h5 class="card-header"><strong>MISSING PERSON DETAILS</strong></h5>
					<% if(profile_missing.photo!=" ") {%>
					<img class="card-img-top" src="<%= profile_missing.missing_photo %>">
					<% } %>
					<div class="card-body" style="text-align: left;">
						<div>Name : <p class="h5 card-title" style="display: inline;"><%=profile_missing.missing_name %></p></div>
						<div>Contact : <p class="h5 card-title" style="display: inline;"><%=profile_missing.missing_contact %></p></div>
						<div>Address : <p class="h5 card-title" style="display: inline;"><%=profile_missing.missing_address %></p></div>
						<div>Place : <p class="h5 card-title" style="display: inline;"><%=profile_missing.missing_city %> <%=profile_missing.missing_state %> <%=profile_missing.missing_pincode %></p></div>
						<div>Descripton : <p class="h5 card-title" style="display: inline;"><%=profile_missing.missing_description %></p></div>
						<div>Missing Since : <p class="h5 card-title" style="display:inline;"><%=profile_missing.missing_date %> <%=profile_missing.missing_time %></p></div>
						<div>Last known Location : <p class="h5 card-title" style="display: inline;"><%=profile_missing.missing_location %></p></div>
						<div>Age : <p class="h5 card-title" style="display: inline;"><%=profile_missing.missing_age %></p></div>
						<div>Gender : <p class="h5 card-title tag" style="display:inline;"><%=profile_missing.missing_gender %></p></div>
					</div>
				</div>
			</div>
			<!--Box 2-->
			<div class="c5">
				<div class="noshadowbox">
					<h5 class="card-header"><strong>REPORTING PERSON DETAILS</strong></h5>
					<div class="card-body" style="text-align: left;">
						<div>Name : <p class="h5" style="display: inline;"><%=profile_missing.reporter_name %></p></div>
						<div>Contact : <p class="h5" style="display: inline;"><%=profile_missing.reporter_contact1 %>  or <%=profile_missing.reporter_contact1 %></p></div>
						<div>Address : <p class="h5 card-title" style="display: inline;"><%=profile_missing.reporter_address %></p></div>
						<div>Pin code : <p class="h5 card-title" style="display: inline;"><%=profile_missing.reporter_pincode %></p></div>
						<div>Relation : <p class="h5 card-title" style="display: inline;"><%=profile_missing.reporter_relation %></p></div>
						<div>Missing Since : <p class="h5 card-title" style="display:inline;"><%=profile_missing.reporter_email %></p></div>
					</div>
				</div>
			</div>
			<!-- NOTE -->
			<div class="c12">
				<div class="royalcontent">
					<p class="h3 bg-warning blockquote">NOTE: Please do search these lists before reporting anyone missing for convinience.</p>
				</div>
			</div>
		</div>
	</div>
	<!-- ==================CONTENT=================== -->

<!-- FOOTER
	================================================== -->
	<% include partials/footer.ejs %>
	<!-- =================/FOOTER===================== -->

</div>


<!-- JAVASCRIPTS
	================================================== -->
	<!-- all-->
	<script type="text/javascript" src="https://code.jquery.com/jquery-2.1.1.min.js"></script>           
	<script src="http://code.jquery.com/ui/1.10.3/jquery-ui.js"></script>
	<!-- all-->
	<script src="/assets/js/modernizr-latest.js"></script>

	<!-- menu & scroll to top -->
	<script src="/assets/js/common.js"></script>


	<!-- CALL Showcase - change 5 from min:5 and max:5 to the number of items you want visible -->
	<script type="text/javascript">
		$(window).load(function(){			
			$('#recent-projects').carouFredSel({
				responsive: true,
				width: '100%',
				auto: true,
				circular	: true,
				infinite	: false,
				prev : {
					button		: "#car_prev",
					key			: "left",
				},
				next : {
					button		: "#car_next",
					key			: "right",
				},
				swipe: {
					onMouse: true,
					onTouch: true
				},
				scroll : 2000,
				items: {
					visible: {
						min: 5,
						max: 5
					}
				}
			});
		});	
	</script>

	<!-- CALL opacity on hover images -->
	<script type="text/javascript">
		$(document).ready(function(){
			$("img.imgOpa").hover(function() {
				$(this).stop().animate({opacity: "0.6"}, 'slow');
			},
			function() {
				$(this).stop().animate({opacity: "1.0"}, 'slow');
			});
		});
	</script>

	<!-- CALL tabs -->
	<script type="text/javascript">
		$(document).ready(function() {	
			$('#tabs li a:not(:first)').addClass('inactive');
			$('.container:not(:first)').hide();	
			$('#tabs li a').click(function(){		
				var t = $(this).attr('href');
		if($(this).hasClass('inactive')){ //added to not animate when active
			$('#tabs li a').addClass('inactive');		
			$(this).removeClass('inactive');
			$('.container').hide();
			$(t).fadeIn('slow');	
		}			
		return false;
	}) //end click
		});
	</script>
	<script type="text/javascript" src="https://code.jquery.com/jquery-2.1.1.min.js"></script>           
	<script src="http://code.jquery.com/ui/1.10.3/jquery-ui.js"></script>
	<script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
	<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js" integrity="sha384-ZMP7rVo3mIykV+2+9J3UJ46jBk0WLaUAdn689aCwoqbBJiSnjAK/l8WvCWPIPm49" crossorigin="anonymous"></script>
	<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js" integrity="sha384-ChfqqxuZUCnJSK3+MXmPNIyE6ZbWh2IMqE241rYiqJxyMiZ6OW/JmZQ5stwEULTy" crossorigin="anonymous"></script>
	<script type="text/javascript" src="https://code.jquery.com/jquery-2.1.1.min.js"></script>           
	<script src="http://code.jquery.com/ui/1.10.3/jquery-ui.js"></script>
</body>
</html>